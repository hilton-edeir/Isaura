<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_activity">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.isaura.ui.me.MeFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/me_fragment" />

    <fragment
        android:id="@+id/navigation_activity"
        android:name="com.isaura.ui.activity.ActivityFragment"
        android:label="@string/title_activity"
        tools:layout="@layout/all_activities_fragment" />

    <fragment
        android:id="@+id/navigation_cleaning"
        android:name="com.isaura.ui.cleaning.CleaningFragment"
        android:label="@string/title_cleaning"
        tools:layout="@layout/cleaning_fragment" />

    <fragment
        android:id="@+id/navigation_utensil"
        android:name="com.isaura.ui.utensil.UtensilsFragment"
        android:label="@string/title_utensil"
        tools:layout="@layout/utensils_fragment" />

</navigation>